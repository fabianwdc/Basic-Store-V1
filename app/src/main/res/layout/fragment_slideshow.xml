<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"

        android:textColor="#F0000000"

        android:textSize="20sp"
        android:layout_marginTop="71dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/text_addres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="8dp"
        android:text="Enter Address"
        android:textAlignment="center"
        android:textColor="#F0000000"

        android:textSize="20sp" />

    <TextView
        android:id="@+id/textViewph"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_addres"

        android:layout_marginTop="67dp"
        android:text="Enter Phone Number"
        android:textAlignment="center"
        android:textColor="#F0000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textViewvnum"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewph"

        android:layout_centerVertical="true"
        android:layout_marginTop="83dp"
        android:text="Enter Vehicle No"
        android:textAlignment="center"
        android:textColor="#F0000000"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="128dp"
        android:text="Address:"
        android:textColor="@color/fui_bgEmail"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="224dp"
        android:text="Phone Number:"
        android:textColor="@color/fui_bgEmail"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="324dp"
        android:text="Vehicle No:"
        android:textColor="@color/fui_bgEmail"
        android:textSize="20sp"/>

</RelativeLayout>